#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vscode-languageserver@8.0.0-next.5/node_modules/vscode-languageserver/bin/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vscode-languageserver@8.0.0-next.5/node_modules/vscode-languageserver/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vscode-languageserver@8.0.0-next.5/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules:/mnt/c/Users/Allen/ui demo/node_modules:/mnt/c/Users/Allen/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vscode-vue-languageservice@0.28.10/node_modules/vscode-vue-languageservice/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vscode-vue-languageservice@0.28.10/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vscode-languageserver@8.0.0-next.5/node_modules/vscode-languageserver/bin/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vscode-languageserver@8.0.0-next.5/node_modules/vscode-languageserver/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vscode-languageserver@8.0.0-next.5/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules:/mnt/c/Users/Allen/ui demo/node_modules:/mnt/c/Users/Allen/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vscode-vue-languageservice@0.28.10/node_modules/vscode-vue-languageservice/node_modules:/mnt/c/Users/Allen/ui demo/viper-ui/node_modules/.pnpm/vscode-vue-languageservice@0.28.10/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vscode-languageserver@8.0.0-next.5/node_modules/vscode-languageserver/bin/installServerIntoExtension" "$@"
else
  exec node  "$basedir/../../../../../vscode-languageserver@8.0.0-next.5/node_modules/vscode-languageserver/bin/installServerIntoExtension" "$@"
fi
